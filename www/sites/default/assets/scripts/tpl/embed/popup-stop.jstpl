<div class="popup-wrapper">

<h4 class="popup-title"><%= _S.ttrunc(this.stop.getAttr("name", "Stop"), 16) %></h4>
<h5 class="popup-placename"><%= _S.ttrunc(_S.htesc(this.stop.getAttr("placename", "")), 24) %></h5>
<% if(this.stop.getAttr("description", false)) { %><p class="popup-description"><%= _S.ttrunc(_S.htesc(this.stop.getAttr("description","")), 64, true) %></p><% } %>
<% if((this.stop.getAttr("description", "").length > 1) || this.stop.getAttr.call(this.stop, Sourcemap.magic_seq.concat(false).slice(1)) !== false) {%><a class="popup-more-link" href="javascript: Sourcemap.embed_stop_details('<%= this.stop.instance_id %>','<%= this.feature.attributes.supplychain_instance_id %>', 0);">More</a><% } %>

</div>
