<h4>Stops</h4>
<table class="sourcemap-tabbed-editor hops">
<thead>
<tr><th>From</th><th>To</th><th>Name</th></tr>
</thead>
<tbody>
<% for(var i=0; i<this.hops.length; i++) { %>
<%     var h = this.hops[i]; %>
<tr><td><%= this.findStop(h.from_stop_id).getLabel() %></td><td><%= this.findStop(h.to_stop_id).getLabel() %></td><td><%= h.getAttr("name", "unnamed") %></td></tr>
<% } %>
</tbody>
</table>
